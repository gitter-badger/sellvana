{% set fieldSetsCtrl = APP.instance('FCom_Sales_Admin_Controller_OrderStateCustom') %}
<script>    
require(['jquery', 'underscore', 'backbone', 'jquery.validate', 'unique'], function($, _, Backbone) {
     
    window.orderCustomStateGridRegister = function(grid) {
            grid.build();
     };
 
});
</script>

<div class="f-admin-main-view">
  <div class="page-header f-admin-page-header">
      <h1 class="f-page-title">
          {#<i class="icon-file"></i>#}
          <span>{{ "Order State Custom" |_ }}</span>
      </h1>
      
      <div class="btn-group">
          <button id="add_new_field" class="btn grid-new btn-primary _modal" type="button"><span>{{ "Add a order state custom" |_ }}</span></button>
      </div>
  </div>

  <div class="f-admin-main">
      {{ THIS.view('core/backbonegrid').set('grid', fieldSetsCtrl.gridConfig())| raw }}
  </div>
</div>




