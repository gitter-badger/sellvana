<div class="f-admin-main-title">
  <div class="page-header">
      <h1 class="pull-left">
          <i class="icon-table"></i>
          {{ THIS.get('title') }}
      </h1>
      <div class="pull-right">
          <div class="btn-group">
              {{ THIS.get('actions') | join(' ') | raw }}
          </div>
      </div>
  </div>
</div>

<div class="f-admin-main-container">
  {{ THIS.view('core/messages') | raw }}
  <div class="box-content">
      {{ THIS.view('core/backbonegrid') | raw }}
  </div>
</div>
