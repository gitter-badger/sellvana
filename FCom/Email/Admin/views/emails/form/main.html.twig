{% set model = THIS.get('model') %}
{% set validator = THIS.get('validator') %}
{% import THIS.view('admin/form-elements').twigName() as forms %}
{% set fieldData = { validator: validator } %}
<fieldset>
    {{ forms.input(fieldData|merge({field:'email', label:'Email'|_})) }}
    {{ forms.boolean(fieldData|merge({field:'unsub_all', label:'Un Subscribe All'|_})) }}
    {{ forms.boolean(fieldData|merge({field:'sub_newsletter', label:'Subscribe Newsletter'|_})) }}
</fieldset>
