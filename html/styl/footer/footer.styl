.fcom-footer
	position fixed
	height footerHeight
	background-color grayColor
	bottom 0
	color whiteColor
	width auto
	right 0

	[class^='fcom-icon'], 
	[class*="fcom-icon"]
		position relative
		margin-right 10px
		display inline-block
		vertical-align top

.fcom-footer-copyright
	position relative
	top 5px
	margin 0px 10px
	display inline-block