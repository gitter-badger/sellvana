<FilesMatch "(attachments)">
Satisfy Any
Order allow,deny
Allow from all
Deny from none
</FilesMatch>

Order allow,deny
Allow from all
#Order deny,allow
#Deny from all
Allow from 64.85.162.30
#Minu
Allow from 24.21.137.218
Allow from 76.109.119.82
Allow from 166.248.137.201
Allow from 178.85.253.152
Allow from 75.95.12.4
Allow from 108.106.63.150
Allow from 108.107.60.160
Allow from 68.242.241.155

AuthName "Denteva"
AuthType Basic
#AuthUserFile .htpasswd
AuthUserFile /var/www/vhosts/dev.unirgy.com/b/denteva/user/.htpasswd
#Require valid-user

Header unset ETag
FileETag None
#Header unset If-Modified-Since
#Header unset If-None-Match

Options +FollowSymLinks -MultiViews

<IfModule mod_rewrite.c>

    SetEnv HTTP_MOD_REWRITE On

    RewriteEngine on

    RewriteRule .* - [E=HTTP_AUTHORIZATION:%{HTTP:Authorization}]

    RewriteCond %{REQUEST_FILENAME} !-f
    RewriteCond %{REQUEST_FILENAME} !-d
    RewriteCond %{REQUEST_FILENAME} !-l

    RewriteRule ^(.*)$ index.php/$1 [L]
</IfModule>
